{
    "name": "onigoetz/reactavel",
    "description": "Use Laravel on ReactPHP",
    "homepage": "https://github.com/onigoetz/reactavel",
    "keywords": ["laravel", "l4", "reactphp"],
    "authors": [
        {
            "name": "Stephane Goetz",
            "email": "onigoetz@onigoetz.ch"
        }
    ],
    "require": {
        "php": ">=5.4.0",
