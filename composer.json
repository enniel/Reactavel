    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0.0",
        "squizlabs/php_codesniffer": ">=1.0.0",
        "phpmd/phpmd": "*",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "Onigoetz\\Reactavel\\": "src/"
        }
    },
    "bin": ["bin/reactavel"]
